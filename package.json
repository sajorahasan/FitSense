{
  "name": "fitsense",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared"
  ],
  "scripts": {
    "check": "biome check --write .",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F @fitsense/mobile dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @fitsense/backend dev",
    "dev:setup": "turbo -F @fitsense/backend dev:setup",
    "mobile": "turbo -F @fitsense/mobile dev",
    "backend": "turbo -F @fitsense/backend dev",
    "clean": "turbo clean && rm -rf node_modules pnpm-lock.yaml",
    "reset": "pnpm clean && pnpm install",
    "setup": "./scripts/setup-dev.sh"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@10.15.0"
}
